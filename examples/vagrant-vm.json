{
	"provider": "vagrant",
	"project": {
		"name": "my-vagrant-project",
		"hostname": "dev.my-vagrant-project.local"
	},
	"vm": {
		"memory": 4096,
		"cpus": 2
	},
	"ports": {
		"frontend": 3000,
		"backend": 3001,
		"postgresql": 5432
	},
	"services": {
		"postgresql": {
			"enabled": true,
			"database": "myapp_dev"
		},
		"redis": {
			"enabled": true
		}
	}
}