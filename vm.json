{
	// Default configuration uses port range 3000-3009
	// Each project should use its own 10-port range to avoid conflicts
	// Note: Your project's "ports" section will completely replace these defaults
	"project": {
		"name": "myproject",
		"hostname": "dev.myproject.local",
		"workspace_path": "/workspace",
		"env_template_path": null,
		"backup_pattern": "*backup*.sql.gz"
	},
	"vm": {
		"box": "bento/ubuntu-22.04",
		"memory": 8192,
		"cpus": 4,
		"user": "vagrant"
	},
	"versions": {
		"node": "22",
		"nvm": "v0.39.7",
		"yarn": "4.9.1"
	},
	// Define your project's ports here (required)
	// Example: "ports": { "frontend": 3020, "backend": 3022, "postgresql": 3025 }
	"ports": {},
	"services": {
		"postgresql": {
			"enabled": false,
			"database": "myproject_dev",
			"user": "postgres",
			"password": "postgres",
			"port": 3005
		},
		"redis": {
			"enabled": false,
			"port": 3006
		},
		"mongodb": {
			"enabled": false,
			"port": 3007
		},
		"docker": {
			"enabled": true
		},
		"headless_browser": {
			"enabled": false,
			"display": ":99",
			"executable_path": "/usr/bin/chromium-browser"
		}
	},
	"npm_packages": [
		"@anthropic-ai/claude-code",
		"npm-check-updates",
		"prettier",
		"eslint"
	],
	"aliases": {},
	"environment": {
		"NODE_ENV": "development",
		"DISPLAY": ":99"
	},
	"terminal": {
		"emoji": "ðŸš€",
		"username": "dev",
		"show_git_branch": true,
		"show_timestamp": false,
		"theme": "dracula"
	}
}